<style>
  .map_venue_symbol_grid {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 1.25rem;
  }
  .flex {
    display: grid;
    grid-template-columns: 30px auto;
    gap: 0.5rem;
    align-content: center;
    justify-content: start;
    img,
    svg {
      display: flex;
    }
  }

  /* --------------CLASSES TIL DE FORSKELLIGE VENUES-------------- */
  .huset {
    .garden_cafe,
    .staircase,
    .cafeteria_and_cafe {
      display: none;
    }
  }
  .blox1 {
    .exhibitions,
    .huset_cafe_and_bar,
    .cafeteria_and_cafe {
      display: none;
    }
  }
  .blox2 {
    .exhibitions,
    .huset_cafe_and_bar,
    .cafeteria_and_cafe,
    .info_desk,
    .entrance_and_exit,
    .garden_cafe {
      display: none;
    }
  }
  .black_diamond {
    .huset_cafe_and_bar,
    .staircase,
    .garden_cafe {
      display: none;
    }
  }
</style>
